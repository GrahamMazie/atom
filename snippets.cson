             * Plugin Version: 0.0.2
             *
             * Equalize height of DOM elements
             */
            $.fn.equalizeHeight = function(callback) {
                var tallest = 0;
                var elements = $(this);
                elements.height("");
                $.each(elements, function() {
                    var thisHeight = parseInt($(this).height());
                    if (thisHeight > tallest) {
                        tallest = thisHeight;
