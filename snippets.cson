             * @param string $needle
             */
            function strpos_array( $haystack, $needle ) {
                foreach( $haystack as $haystack_item ) {
                    if ( strpos( $needle, $haystack_item ) !== false ) {
                        return true;
                    }
                }
                return false;
            }
